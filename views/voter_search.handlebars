<div class="container">
  <div class="row" id="vote-search">
    <label class="filter-menu" id="filter">
      <h2>VOTER SEARCH </h2></label>
  </div>
  <form id="voter-search">
    <div class="form-group row">
      <div class="col-11">
        <div class="form-group row">
          <label for="first-name-search" class="col-3 col-form-label filter-menu one">First Name</label>
          <div class="col-9" id="first-name">
            <input type="text" class="form-control filter-menu" id="first-name-search">
          </div>
        </div>
        <div class="form-group row">
          <label for="last-name-search" class="col-3 col-form-label filter-menu one">Last Name</label>
          <div class="col-9" id="last-name">
            <input type="text" class="form-control filter-menu" id="last-name-search">
          </div>
        </div>
      </div>
      <div class="col-1" id="search">
        <button id="voter-search-button" class="btn btn-primary filter-menu" type="button">
          <span class="glyphicon glyphicon-search"></span>
        </button>
      </div>
    </div>
  </form>
  <div class="jumbotron">
    <div class="row-col-10">
      <br>
      <h2>RESULTS</h2>
    </div>
    <div class="row-col-10 flex-table">
      <table class="table">
        <thead>
          <tr>
            <th class="first-name">First Name</th>
            <th class="last-name">Last Name</th>
            <th class="address">Address</th>
            <th class="city">City</th>
            <th class="zip">Zip</th>
            <th class="party">Party</th>
            <th class="voter-details">Voter Details</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
    <br>
    <!-- Knocking Button -->
  </div>
</div>